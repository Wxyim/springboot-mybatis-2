<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.dao.StuDao">
    <select id="queryStuById" resultType="com.example.demo.entity.Stu">
        select id, name, score from stu where id = #{id}
    </select>
</mapper>